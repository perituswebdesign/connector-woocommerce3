{
  "name": "peritus/woo-jtl-connector",
  "type": "app",
  "description": "WooCommerce Connector based on jtl/connector",
  "keywords": [
    "jtl",
    "connector",
    "WooCommerce"
  ],
  "homepage": "http://www.jtl-software.de",
  "license": "GPL",
  "authors": [
    {
      "name": "Jan Weskamp",
      "email": "jan.weskamp@jtl-software.com",
      "homepage": "http://www.jtl-software.de",
      "role": "Developer"
    }
  ],
  "config": {
    "platform": {
      "php": "7.1.3"
    }
  },
  "require": {
    "php": ">=7.1.3",
    "jtl/connector": "^2.7",
    "symfony/yaml": "^3.4",
    "twbs/bootstrap": "^4.3",
    "theiconic/name-parser": "^0.1.1",
    "php-units-of-measure/php-units-of-measure": "^2.1"
  },
  "require-dev": {
    "phpunit/phpunit": "5.7.*",
    "php-mock/php-mock": "^2.2"
  },
  "autoload": {
    "psr-4": {
      "JtlWooCommerceConnector\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "JtlWooCommerceConnector\\Tests\\": "tests/"
    }
  }
}
